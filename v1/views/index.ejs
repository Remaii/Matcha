<% include partial/header %>

<div class="row">
<% if (locals.session['login'] != undefined) {
    for (var i = 0; locals.session['allprof'][i]; i++) { %>
    <a href="/profile/<%= locals.session['allprof'][i][1] %>">
        <div class="col-xs-4 col-md-4">
            <div class="col-md-12 col-xs-12">
                <br><img class="col-md-12 col-xs-12 img-circle" src="<% if (locals.session['allprof'][i][0] != null) { %><%= '/asset/uploads/' + locals.session['allprof'][i][0] %><% } %>">
            </div>
            <div class="col-md-12 col-xs-12">
                <h5 class="text-center"><%= locals.session['allprof'][i][1] %></h5>
            </div>
        </div>
    </a>
    <% } 
} else { %>
    <div class="col-md-4 col-md-offset-4">
        <a href="/register"><h2 class="text-center">Inscrit-toi !</h2></a>
    </div>
<% } %>
</div>

<% include partial/footer %>