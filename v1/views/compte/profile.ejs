<% include ../partial/header %>

<% if (locals.session['myinfo']) { %>
	<img src=<%= '/asset/uploads/' + locals.session['mypic'][0] %> class="col-md-offset-4 col-xs-offset-4 col-md-4 col-xs-4 img-circle">
	<div class="row">
		<div>
			<p class="text-center"><%= locals.session['myinfo'][0] %> <%= locals.session['myinfo'][1] %> <%= locals.session['myinfo'][2] %> ans</p>
			<p class="text-center"><%= locals.session['myinfo'][3] %> <%= locals.session['myinfo'][4] %></p>
			<% if (locals.session['mytag'][0]) { %>
				<p class="text-center">
				<% for (var i = 0; locals.session['mytag'][i]; i++) { %>
					<% if (locals.session['mytag'][i] != ' ') { %>
						<%= '#' + locals.session['mytag'][i] %>
					<% } %>
				<% } %>
				</p>
			<% } %>
			<p class="text-center"><%= locals.session['myinfo'][5] %></p>
		</div>
	</div>
<% } %>

<% include ../partial/footer %>