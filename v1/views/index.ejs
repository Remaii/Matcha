<% include partial/header %>

<div class="row">
<% if (locals.session['login'] != undefined) {
        for (var i = 0; locals.session['allprof'][i]; i++) { %>
            <div class="col-xs-4 col-md-4">
                <div class="col-md-offset-3">
                    <img class="col-md-10 col-xs-10 img-circle" src="<% if (locals.session['allprof'][i][7] != null) { %><%= '/asset/uploads/' + locals.session['allprof'][i][7] %><% } %>">
                </div>
                
                    <a href="/profile/<%= locals.session['allprof'][i][6] %>"><h4 class="text-center"><%= locals.session['allprof'][i][0] %> <%= locals.session['allprof'][i][1]%> <% if (locals.session['allprof'][i][2] != undefined) { %></a>
                    <%= locals.session['allprof'][i][2] %> ans</h4>
                <% } %>
                <p class="text-center"><%= locals.session['allprof'][i][3] %></p>
                <p class="text-center"><%= locals.session['allprof'][i][4] %></p>
                <p class="text-center"><% if (locals.session['allprof'][i][5]) { for(var j = 0; locals.session['allprof'][i][5][j]; j++) { %>
                        <%= '#' + locals.session['allprof'][i][5][j] %>
                   <% }} %></p>
            </div>
        <% } 
} else { %>
    <div class="col-md-4 col-md-offset-4">
        <a href="/register"><h2 class="text-center">Inscrit-toi !</h2></a>
    </div>
<% } %>
</div>

<% include partial/footer %>